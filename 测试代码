std::cout << "=== æµ‹è¯•1ï¼šåŸºæœ¬åŠŸèƒ½æµ‹è¯• ===\n";
    
    LinkedList<int> list;
    
    // 1. æµ‹è¯•ç©ºé“¾è¡¨
    std::cout << "1. ç©ºé“¾è¡¨æµ‹è¯•:\n";
    std::cout << "   size = " << list.get_size() << " (åº”ä¸º0)\n";
    std::cout << "   isempty = " << list.isempty() << " (åº”ä¸º1/true)\n";
    assert(list.get_size() == 0);
    assert(list.isempty());
    
    // 2. æµ‹è¯•push_front
    std::cout << "\n2. push_frontæµ‹è¯•:\n";
    list.push_front(10);
    list.push_front(20);
    list.push_front(30);
    std::cout << "   æ·»åŠ 30,20,10å: ";
    list.print();  // åº”ä¸º: 30 20 10
    assert(list.get_size() == 3);
    assert(list.front() == 30);
    assert(list.back() == 10);
    
    // 3. æµ‹è¯•push_back
    std::cout << "\n3. push_backæµ‹è¯•:\n";
    list.push_back(40);
    list.push_back(50);
    std::cout << "   å†æ·»åŠ 40,50å: ";
    list.print();  // åº”ä¸º: 30 20 10 40 50
    assert(list.get_size() == 5);
    assert(list.back() == 50);
    
    // 4. æµ‹è¯•pop_front
    std::cout << "\n4. pop_frontæµ‹è¯•:\n";
    int val1 = list.pop_front();
    std::cout << "   å¼¹å‡º: " << val1 << " (åº”ä¸º30)\n";
    std::cout << "   å½“å‰: ";
    list.print();  // åº”ä¸º: 20 10 40 50
    assert(val1 == 30);
    assert(list.get_size() == 4);
    assert(list.front() == 20);
    
    // 5. æµ‹è¯•pop_back
    std::cout << "\n5. pop_backæµ‹è¯•:\n";
    int val2 = list.pop_back();
    std::cout << "   å¼¹å‡º: " << val2 << " (åº”ä¸º50)\n";
    std::cout << "   å½“å‰: ";
    list.print();  // åº”ä¸º: 20 10 40
    assert(val2 == 50);
    assert(list.get_size() == 3);
    assert(list.back() == 40);
    
    // 6. æµ‹è¯•find_index
    std::cout << "\n6. find_indexæµ‹è¯•:\n";
    size_t idx1 = list.find_index(10);
    size_t idx2 = list.find_index(20);
    size_t idx3 = list.find_index(999);  // ä¸å­˜åœ¨
    std::cout << "   10çš„ä½ç½®: " << idx1 << " (åº”ä¸º1)\n";
    std::cout << "   20çš„ä½ç½®: " << idx2 << " (åº”ä¸º0)\n";
    std::cout << "   999çš„ä½ç½®: " << idx3 << " (åº”ä¸º88888888)\n";
    assert(idx1 == 1);
    assert(idx2 == 0);
    assert(idx3 == 88888888);
    
    // 7. æµ‹è¯•reverse
    std::cout << "\n7. reverseæµ‹è¯•:\n";
    std::cout << "   åè½¬å‰: ";
    list.print();  // 20 10 40
    list.reverse();
    std::cout << "   åè½¬å: ";
    list.print();  // åº”ä¸º: 40 10 20
    assert(list.front() == 40);
    assert(list.back() == 20);
    
    // 8. æµ‹è¯•æ‹·è´æ„é€ 
    std::cout << "\n8. æ‹·è´æ„é€ æµ‹è¯•:\n";
    LinkedList<int> list2 = list;  // æ‹·è´æ„é€ 
    std::cout << "   åŸé“¾è¡¨: ";
    list.print();   // 40 10 20
    std::cout << "   æ‹·è´å: ";
    list2.print();  // åº”ä¸º: 40 10 20
    assert(list2.get_size() == 3);
    assert(list2.front() == 40);
    assert(list2.back() == 20);
    
    // 9. æµ‹è¯•æ‹·è´èµ‹å€¼
    std::cout << "\n9. æ‹·è´èµ‹å€¼æµ‹è¯•:\n";
    LinkedList<int> list3;
    list3 = list;  // æ‹·è´èµ‹å€¼
    std::cout << "   èµ‹å€¼å: ";
    list3.print();  // åº”ä¸º: 40 10 20
    assert(list3.get_size() == 3);
    
    // 10. æµ‹è¯•ç§»åŠ¨æ„é€ 
    std::cout << "\n10. ç§»åŠ¨æ„é€ æµ‹è¯•:\n";
    LinkedList<int> list4 = std::move(list3);
    std::cout << "   ç§»åŠ¨åæ–°é“¾è¡¨: ";
    list4.print();  // åº”ä¸º: 40 10 20
    std::cout << "   ç§»åŠ¨ååŸé“¾è¡¨å¤§å°: " << list3.get_size() << " (åº”ä¸º0)\n";
    assert(list4.get_size() == 3);
    assert(list3.isempty());
    
    // 11. æµ‹è¯•ç§»åŠ¨èµ‹å€¼
    std::cout << "\n11. ç§»åŠ¨èµ‹å€¼æµ‹è¯•:\n";
    LinkedList<int> list5;
    list5 = std::move(list4);
    std::cout << "   ç§»åŠ¨èµ‹å€¼å: ";
    list5.print();  // åº”ä¸º: 40 10 20
    std::cout << "   ç§»åŠ¨ååŸé“¾è¡¨å¤§å°: " << list4.get_size() << " (åº”ä¸º0)\n";
    assert(list5.get_size() == 3);
    assert(list4.isempty());
    
    // 12. æµ‹è¯•clear
    std::cout << "\n12. clearæµ‹è¯•:\n";
    list5.clear();
    std::cout << "   æ¸…ç©ºåå¤§å°: " << list5.get_size() << " (åº”ä¸º0)\n";
    assert(list5.isempty());
    
    std::cout << "\nâœ… æ‰€æœ‰æµ‹è¯•é€šè¿‡ï¼\n";

    std::cout << "=== è¾¹ç•Œæƒ…å†µæµ‹è¯• ===\n\n";
    
    LinkedList<int> list;
    
    // æµ‹è¯•1ï¼šå®Œå…¨ç©ºé“¾è¡¨çš„æ‰€æœ‰æ“ä½œ
    std::cout << "æµ‹è¯•1: ç©ºé“¾è¡¨æ“ä½œ\n";
    std::cout << "-----------------\n";
    
    // 1.1 front() åº”è¯¥æŠ›å¼‚å¸¸
    std::cout << "1.1 æµ‹è¯• front() åœ¨ç©ºé“¾è¡¨:\n";
    try {
        int val = list.front();
        std::cout << "   âŒ é”™è¯¯ï¼šåº”è¯¥æŠ›å¼‚å¸¸ï¼Œä½†å¾—åˆ°å€¼: " << val << "\n";
    } catch (const std::out_of_range& e) {
        std::cout << "   âœ… æ­£ç¡®æŠ›å‡ºå¼‚å¸¸: " << e.what() << "\n";
    }
    
    // 1.2 back() åº”è¯¥æŠ›å¼‚å¸¸
    std::cout << "\n1.2 æµ‹è¯• back() åœ¨ç©ºé“¾è¡¨:\n";
    try {
        int val = list.back();
        std::cout << "   âŒ é”™è¯¯ï¼šåº”è¯¥æŠ›å¼‚å¸¸ï¼Œä½†å¾—åˆ°å€¼: " << val << "\n";
    } catch (const std::out_of_range& e) {
        std::cout << "   âœ… æ­£ç¡®æŠ›å‡ºå¼‚å¸¸: " << e.what() << "\n";
    }
    
    // 1.3 pop_front() åº”è¯¥æŠ›å¼‚å¸¸
    std::cout << "\n1.3 æµ‹è¯• pop_front() åœ¨ç©ºé“¾è¡¨:\n";
    try {
        int val = list.pop_front();
        std::cout << "   âŒ é”™è¯¯ï¼šåº”è¯¥æŠ›å¼‚å¸¸ï¼Œä½†å¾—åˆ°å€¼: " << val << "\n";
    } catch (const std::out_of_range& e) {
        std::cout << "   âœ… æ­£ç¡®æŠ›å‡ºå¼‚å¸¸: " << e.what() << "\n";
    }
    
    // 1.4 pop_back() åº”è¯¥æŠ›å¼‚å¸¸
    std::cout << "\n1.4 æµ‹è¯• pop_back() åœ¨ç©ºé“¾è¡¨:\n";
    try {
        int val = list.pop_back();
        std::cout << "   âŒ é”™è¯¯ï¼šåº”è¯¥æŠ›å¼‚å¸¸ï¼Œä½†å¾—åˆ°å€¼: " << val << "\n";
    } catch (const std::out_of_range& e) {
        std::cout << "   âœ… æ­£ç¡®æŠ›å‡ºå¼‚å¸¸: " << e.what() << "\n";
    }
    
    // æµ‹è¯•2ï¼šåªæœ‰ä¸€ä¸ªå…ƒç´ çš„é“¾è¡¨
    std::cout << "\n\næµ‹è¯•2: å•å…ƒç´ é“¾è¡¨\n";
    std::cout << "-----------------\n";
    
    list.push_back(42);  // ç°åœ¨é“¾è¡¨: [42]
    
    // 2.1 front() å’Œ back() åº”è¯¥ç›¸åŒ
    std::cout << "2.1 æµ‹è¯•å•å…ƒç´ é“¾è¡¨çš„ front() å’Œ back():\n";
    std::cout << "   front() = " << list.front() << "\n";
    std::cout << "   back() = " << list.back() << "\n";
    if (list.front() == list.back()) {
        std::cout << "   âœ… front() å’Œ back() ç›¸åŒ\n";
    } else {
        std::cout << "   âŒ é”™è¯¯ï¼šfront() å’Œ back() åº”è¯¥ç›¸åŒ\n";
    }
    
    // 2.2 pop_front() åé“¾è¡¨å˜ç©º
    std::cout << "\n2.2 æµ‹è¯• pop_front() å:\n";
    int val = list.pop_front();
    std::cout << "   å¼¹å‡ºå€¼: " << val << " (åº”ä¸º42)\n";
    std::cout << "   å¼¹å‡ºå size: " << list.get_size() << " (åº”ä¸º0)\n";
    std::cout << "   å¼¹å‡ºå isempty: " << list.isempty() << " (åº”ä¸º1)\n";
    
    // 2.3 å†æ¬¡æµ‹è¯•ç©ºé“¾è¡¨ï¼ˆç¡®ä¿pop_frontåçœŸçš„ç©ºäº†ï¼‰
    std::cout << "\n2.3 æµ‹è¯• pop_front() åå†æ¬¡æ“ä½œ:\n";
    try {
        list.front();
        std::cout << "   âŒ é”™è¯¯ï¼šåº”è¯¥æŠ›å¼‚å¸¸ä½†æ²¡æŠ›\n";
    } catch (const std::out_of_range& e) {
        std::cout << "   âœ… æ­£ç¡®æŠ›å‡ºå¼‚å¸¸\n";
    }
    
    // æµ‹è¯•3ï¼šä¸¤ä¸ªå…ƒç´ çš„é“¾è¡¨
    std::cout << "\n\næµ‹è¯•3: åŒå…ƒç´ é“¾è¡¨\n";
    std::cout << "-----------------\n";
    
    list.push_back(100);
    list.push_back(200);  // é“¾è¡¨: [100, 200]
    
    std::cout << "3.1 æµ‹è¯•åŒå…ƒç´ é“¾è¡¨:\n";
    std::cout << "   front() = " << list.front() << " (åº”ä¸º100)\n";
    std::cout << "   back() = " << list.back() << " (åº”ä¸º200)\n";
    std::cout << "   size = " << list.get_size() << " (åº”ä¸º2)\n";
    
    // 3.2 pop_back() æµ‹è¯•
    std::cout << "\n3.2 æµ‹è¯• pop_back():\n";
    int back_val = list.pop_back();
    std::cout << "   å¼¹å‡ºå€¼: " << back_val << " (åº”ä¸º200)\n";
    std::cout << "   å¼¹å‡ºå size: " << list.get_size() << " (åº”ä¸º1)\n";
    std::cout << "   å¼¹å‡ºå front = back = " << list.front() << " (åº”ä¸º100)\n";
    
    // æµ‹è¯•4ï¼šå¤§é‡æ“ä½œåçš„è¾¹ç•Œ
    std::cout << "\n\næµ‹è¯•4: å¤§é‡æ“ä½œè¾¹ç•Œ\n";
    std::cout << "-------------------\n";
    
    list.clear();
    
    // 4.1 è¿ç»­push/pop
    std::cout << "4.1 è¿ç»­push/popæµ‹è¯•:\n";
    for (int i = 0; i < 10; ++i) {
        list.push_front(i);
    }
    std::cout << "   æ·»åŠ 10ä¸ªå…ƒç´ å size: " << list.get_size() << "\n";
    
    // è¿ç»­popç›´åˆ°ç©º
    int pop_count = 0;
    while (!list.isempty()) {
        list.pop_front();
        pop_count++;
    }
    std::cout << "   å¼¹å‡º " << pop_count << " ä¸ªå…ƒç´ å size: " << list.get_size() << "\n";
    
    // 4.2 å†æ¬¡æµ‹è¯•ç©ºé“¾è¡¨æ“ä½œ
    std::cout << "\n4.2 æ¸…ç©ºåæµ‹è¯•:\n";
    try {
        list.pop_back();
        std::cout << "   âŒ é”™è¯¯ï¼šåº”è¯¥æŠ›å¼‚å¸¸ä½†æ²¡æŠ›\n";
    } catch (const std::out_of_range& e) {
        std::cout << "   âœ… æ­£ç¡®æŠ›å‡ºå¼‚å¸¸\n";
    }
    
    // æµ‹è¯•5ï¼šæ‹·è´å’Œç§»åŠ¨çš„è¾¹ç•Œ
    std::cout << "\n\næµ‹è¯•5: æ‹·è´ç§»åŠ¨è¾¹ç•Œ\n";
    std::cout << "-------------------\n";
    
    LinkedList<int> source;
    source.push_back(1);
    source.push_back(2);
    
    // 5.1 æ‹·è´ç©ºé“¾è¡¨
    std::cout << "5.1 æ‹·è´éç©ºé“¾è¡¨:\n";
    LinkedList<int> copy = source;  // æ‹·è´æ„é€ 
    std::cout << "   æºé“¾è¡¨ size: " << source.get_size() << "\n";
    std::cout << "   æ‹·è´é“¾è¡¨ size: " << copy.get_size() << "\n";
    
    // 5.2 æ¸…ç©ºæºé“¾è¡¨ï¼Œæµ‹è¯•æ‹·è´æ˜¯å¦ç‹¬ç«‹
    std::cout << "\n5.2 æµ‹è¯•æ‹·è´ç‹¬ç«‹æ€§:\n";
    source.clear();
    std::cout << "   æ¸…ç©ºæºé“¾è¡¨å:\n";
    std::cout << "     æºé“¾è¡¨ size: " << source.get_size() << " (åº”ä¸º0)\n";
    std::cout << "     æ‹·è´é“¾è¡¨ size: " << copy.get_size() << " (åº”ä¸º2)\n";
    
    if (copy.get_size() == 2) {
        std::cout << "   âœ… æ‹·è´æ˜¯ç‹¬ç«‹çš„\n";
    } else {
        std::cout << "   âŒ é”™è¯¯ï¼šæ‹·è´ä¸ç‹¬ç«‹\n";
    }
    
    // æµ‹è¯•6ï¼šreverseè¾¹ç•Œ
    std::cout << "\n\næµ‹è¯•6: reverseè¾¹ç•Œ\n";
    std::cout << "-------------------\n";
    
    LinkedList<int> rev_list;
    
    // 6.1 ç©ºé“¾è¡¨reverse
    std::cout << "6.1 ç©ºé“¾è¡¨reverse:\n";
    rev_list.reverse();  // åº”è¯¥ä»€ä¹ˆéƒ½ä¸åšï¼Œä¸å´©æºƒ
    std::cout << "   âœ… ç©ºé“¾è¡¨reverseæˆåŠŸ\n";
    
    // 6.2 å•å…ƒç´ é“¾è¡¨reverse
    std::cout << "\n6.2 å•å…ƒç´ é“¾è¡¨reverse:\n";
    rev_list.push_back(999);
    rev_list.reverse();
    std::cout << "   åè½¬å front: " << rev_list.front() << " (åº”ä¸º999)\n";
    std::cout << "   åè½¬å back: " << rev_list.back() << " (åº”ä¸º999)\n";
    
    // 6.3 åŒå…ƒç´ é“¾è¡¨reverse
    std::cout << "\n6.3 åŒå…ƒç´ é“¾è¡¨reverse:\n";
    rev_list.clear();
    rev_list.push_back(1);
    rev_list.push_back(2);
    std::cout << "   åè½¬å‰: ";
    rev_list.print();
    rev_list.reverse();
    std::cout << "   åè½¬å: ";
    rev_list.print();
    std::cout << "   åè½¬å front: " << rev_list.front() << " (åº”ä¸º2)\n";
    std::cout << "   åè½¬å back: " << rev_list.back() << " (åº”ä¸º1)\n";
    
    std::cout << "\nğŸ‰ æ‰€æœ‰è¾¹ç•Œæµ‹è¯•å®Œæˆï¼\n";